   "id": "debf4639-447f-48aa-950d-6fa1cf05ce07",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter your date of births(YYYY-MM-DD) :  2002-12-11\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Your age is 22 years\n"
     ]
    }
   ],
   "source": [
    "import datetime\n",
    "\n",
    "now = datetime.datetime.now()\n",
    "\n",
    "\n",
    "date = input('Enter your date of births(YYYY-MM-DD) : ')\n",
    "\n",
    "date1 = datetime.datetime.strptime(date  , '%Y-%m-%d')\n",
    "\n",
    "difference = now - date1\n",
    "years = difference.days //365\n",
    "print(f'Your age is {years } years')"
   ]
  }
